<div class="single_carousel">
  <div class="single_testmonial text-center">
    <div class="quote">
      <img src="{{ base_path ~ directory }}/images/quote.svg"/>
    </div>
    {{ elements.field_content }}
    <div class="testmonial_author">
      <div class="thumb">
        {{ elements.field_profile_image }}
      </div>
      <h3>{{ elements.field_author_name }}</h3>
      <span>{{ elements.field_author_title }}</span>
    </div>
  </div>
</div>
